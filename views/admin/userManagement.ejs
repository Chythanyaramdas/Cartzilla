                <tbody>
                  <% if(users.length > 0){
                    for(i = 0;i < users.length;i++){
                        %>
                        <tr>
                            <td><%= i+1 %></td>
                              <td><%= users[i].name%></td>
    
                            <td><%= users[i].email %></td>
                            
                            <% if(users[i].isBlocked){ %>
                                <td><button type="button" class="btn btn-outline-primary" onclick="return confirm('Are you sure you want to Unblock this user?')">
